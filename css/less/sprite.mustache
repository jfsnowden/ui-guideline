{{#sprites}}

[[{{name}}]] {
        background-image: url(../img/main-ui/{{{escaped_image}}});
        background-repeat: no-repeat;
        background-position: {{px.offset_x}} {{px.offset_y}};
        width: {{px.width}};
        height: {{px.height}};
        display: inline-block;
        text-align: center;
        vertical-align: middle;
    }

{{/sprites}}